    "optimize:images": "node js/optimize-images.js",
    "add:lazyload": "node js/add-lazyload.js"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@lhci/cli": "^0.12.0",
    "@squoosh/cli": "^0.5.0",
    "cheerio": "^1.0.0-rc.12",
    "clean-css-cli": "^5.6.0",
    "cssnano": "^6.0.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.2.7",
    "html-minifier-terser": "^7.0.0",
    "lighthouse": "^10.0.0",
    "postcss": "^8.4.25",
    "sharp": "^0.32.6",
    "serve": "^14.2.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/muktiswarup/vishal-testing.git"
