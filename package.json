{
  "name": "vishal-project-optimizer",
  "version": "1.0.0",
  "private": true,
  "description": "Website optimization and performance monitoring for vishal project",
  "scripts": {
    "optimize": "node js/optimize.js",
    "serve:optimized": "npx serve optimized -l 8080",
    "lhci": "npx lhci autorun",
    "prune-images": "node js/prune-unused-images.js --root optimized",
